<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:paddingTop="3dp"
	android:paddingBottom="3dp"
	android:paddingLeft="10dp" android:gravity="bottom"
	android:paddingRight="10dp" >
	<LinearLayout android:orientation="vertical"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_weight="1">
		<TextView android:id="@+id/block_blackwhitelist_black_listitem_nicename_txt"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:textColor="#000000" android:textSize="16dp"/>
		<TextView android:id="@+id/block_blackwhitelist_black_listitem_phone_txt"
		    android:layout_marginTop="3dp"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:textColor="@color/txt_label_black" android:textSize="12dp"/>

	</LinearLayout>
	<TextView android:id="@+id/block_blackwhitelist_black_listitem_attr_txt"
		android:layout_height="wrap_content" android:layout_width="wrap_content"
		android:textColor="#016fae"  android:textSize="12dp"/>
</LinearLayout>
